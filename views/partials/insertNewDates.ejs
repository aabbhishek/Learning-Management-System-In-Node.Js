<div id="insertNewDate" class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="card">
        <div class="card-header">
        Insert Lesson dates
        </div>
        <div class="card-body">
          <div class="form-group">
            <label for="dateOne">Enter date</label>
            <input type="date" class="form-control" id="dateOne" >
          </div>
          <div class="form-group">
            <label for="about">About</label>
            <input type="text" class="form-control" id="about" >
          </div>

          <button type="button" class="btn btn-block" style="background:#00E676;" courseName="<%= courseName %>" onclick="insertNewDocument();">Insert</button>
        </div>
      </div>

    </div>
  </div>
</div>
