<form method="post" action="/">
  <div class="form-group">
    <label for="email">Email address</label>
    <input type="email" class="form-control under_text" id="email" aria-describedby="emailHelp" placeholder="Enter email" onkeydown="onChnage();" name="emailId">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>

  <div class="form-group">
    <label for="password">Password</label>
    <input type="password" class="form-control under_text" id="password" placeholder="Password" onkeydown="onChnage();" name="password"
  </div>

  <div class="form-group">
    <label for="password">Confirm Password</label>
    <input type="password" class="form-control under_text" id="cpassword" placeholder="Password" onkeydown="onChnage();">
  </div>
  

  <button type="submit" id="singIn" class="btn btn-primary" style="background-color:#2CC9CC;border:none;width:40%;" onclick="event.preventDefault();  switchInputTwo();"><i class="fas fa-sign-in-alt"></i> Sing In</button>
  <button type="submit" id="singAjax" class="btn btn-primary" style="background-color:#2CC9CC;border:none;width:40%;" onclick="event.preventDefault(); sendRegi();"><i class="fas fa-sign-in-alt"></i> Submit</button>
</form>
